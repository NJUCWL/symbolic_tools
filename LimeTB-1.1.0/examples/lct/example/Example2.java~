package example;
public class Example2
{

		public static void foo (int a, int b) {
			if (a > b)
				System.out.println("a > b");
      else if (a == 5421)
        System.out.println(a);
      else
        System.out.println(a);      
		}   

    public static void main(String args[]) 
    {
				int x = 5;
				int y = 30;

				Example1.foo(x, y);

				System.out.println("end.");				
    }
}
